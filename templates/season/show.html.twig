{% extends 'base.html.twig' %}

{% block title %}Season{% endblock %}

{% block body %}
    <h1>{{ season.program.title }}</h1>
    <h2>Season {{ season.id }}</h2>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ season.id }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ season.number }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ season.year }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ season.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_season_edit', {'id': season.id}) }}" class="link">edit</a>
    {{ include('season/_delete_form.html.twig') }}
    <a href="{{ path('app_season_index') }}" class="link">back to list</a>
{% endblock %}
